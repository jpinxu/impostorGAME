<?xml version='1.0' encoding='utf-8'?>
<widget id="com.imposwho.app" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>imposWHO</name>
    <description>Juego de rol social - Encuentra al impostor (v1.1: League of Legends completo + Primer jugador)</description>
</invoke>
    <author email="dev@imposwho.com" href="https://github.com/jpinxu/imposWHO">
        imposWHO Team
    </author>
    <icon src="res/icon/icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="24" />
        <preference name="android-targetSdkVersion" value="35" />
        <preference name="Orientation" value="portrait" />
        <preference name="AndroidXEnabled" value="true" />
        <config-file target="AndroidManifest.xml" parent="/manifest" mode="merge">
            <uses-permission android:name="android.permission.CAMERA" />
        </config-file>
    </platform>
    <preference name="DisallowOverscroll" value="true" />
</widget>
